#include <iostream>
#include <string>
using namespace std;

int main() {
    string nomes[6];
    string buscar;
    bool encontrado = false;

    cout << "Digite 6 nomes:\n";

    for (int i = 0; i < 6; i++) {
        cin >> nomes[i];
    }

    cout << "\nDigite um nome para buscar: ";
    cin >> buscar;

    // indo buscar no vetor
    for (int i = 0; i < 6; i++) {
        if (nomes[i] == busca) {
            encontrado = true;
            break;
        }
    }

    if (encontrado) {
        cout << "\nO nome \"" << buscar << "\" esta no vetor.\n";
    } else {
        cout << "\nO nome \"" << buscar << "\" nao esta no vetor.\n";
    }

    return 0;
}
