#include <iostream>
using namespace std;

int main() {
    int vetInt[5];
    int entradaNum;

    cout << "Digite 5 numeros: ";

    for (int i = 0; i < 5; i++) {
        cin >> entradaNum;
        vetInt[i] = entradaNum;
    }

    cout << "Os numeros digitados foram:" << endl;

    for (int i = 0; i < 5; i++) {
        cout << vetInt[i] << endl;
    }

    return 0;
}
